<h1> {{ title }} </h1>

<ul class="demo-list-icon mdl-list">
  <li class="mdl-list__item">
    <span class="mdl-list__item-primary-content">
      <i class="material-icons mdl-list__item-icon">flight</i>
      <pre>Provider</pre>
      <span class="mdl-list__item agony">
      <pre>Score</pre>
      </span>
      <span class="mdl-list__item price">
      <pre>Price</pre>
      </span>
    </span>
  </li>
</ul>

<ul class="demo-list-icon mdl-list">
   {{#each sortedFlights}}
  <li class="mdl-list__item">
    <span class="mdl-list__item-primary-content">
      <i class="material-icons mdl-list__item-icon">flight</i>
      {{ provider }}
      <span class="mdl-list__item agony">
      {{ agony }}
      </span>
      <span class="mdl-list__item price">
         ${{ price }}
      </span>
    </span>
  </li>
     {{/each}}
</ul>